{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1"
        },
        {
            "src": "/storage/(.*)",
            "dest": "public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ],
    "installCommand": "composer install --no-dev --optimize-autoloader",
    "buildCommand": "echo 'Build complete'",
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false"
    }
}